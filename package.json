{
  "name": "food-tracker",
  "author": "Raimon Espasa",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tsc && next lint",
    "lint:fix": "tsc && next lint --fix",
    "test": "TZ=UTC vitest --watch",
    "test:ci": "TZ=UTC vitest run --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:push": "prisma db push --skip-generate",
    "prisma:pull": "prisma db pull --force",
    "prisma:seed": "prisma db seed",
    "prisma:studio": "prisma studio",
    "prisma:reset": "npm run prisma:migrate && npm run prisma:seed"
  },
  "postinstall": "npm run prisma:generate",
  "prisma": {
    "seed": "node prisma/seed/seed.mjs"
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "2.5.0",
    "@emotion/react": "11.11.0",
    "@emotion/styled": "11.11.0",
    "@mui/icons-material": "5.11.16",
    "@mui/material": "5.12.3",
    "@mui/x-date-pickers": "6.3.1",
    "@prisma/client": "4.13.0",
    "@supabase/supabase-js": "2.21.0",
    "classcat": "5.0.4",
    "dayjs": "1.11.7",
    "jsonwebtoken": "9.0.0",
    "next": "13.4.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "0.32.1",
    "swr": "2.1.5"
  },
  "devDependencies": {
    "@types/jsonwebtoken": "9.0.1",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@vitest/coverage-c8": "0.29.7",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "prisma": "4.13.0",
    "sass": "1.62.1",
    "typescript": "5.0.4",
    "vitest": "0.29.7"
  }
}
